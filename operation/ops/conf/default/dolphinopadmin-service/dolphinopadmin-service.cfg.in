[dolphinopadmin-service]
debug	        = %(web_debug)s
logs_dir        = %(log_dir_base)s/dolphinopadmin-service
data_dir        = %(data_dir_base)s/dolphinopadmin-service
static_root     = /var/app/enabled/dolphinopadmin-service
media_root      = /var/app/data/dolphinopadmin-service
request_timeout = 1.0
max_retry       = 3
http_max_age    = 86400
backend_timeout = 5
max_ip_count	= 10
max_sim_count	= 4
domain          =  %(url)s
admin_production = %(admin_production_conf)s
admin_db =  %(admin_db_conf)s
admin_env_ec2 = %(admin_env_ec2_conf)s
admin_env_china = %(admin_env_china_conf)s
admin_env_local = %(admin_env_local_conf)s
web_env_ec2 = %(web_env_ec2_conf)s
web_env_china = %(web_env_china_conf)s
web_env_local = %(web_env_local_conf)s
domain_env_ec2 = %(domain_env_ec2_conf)s
domain_env_china = %(domain_env_china_conf)s
domain_env_local = %(domain_env_local_conf)s
server          = %(server_conf)s
