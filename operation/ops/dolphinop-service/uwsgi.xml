<uwsgi>
    <socket>:8000</socket> 
    <env>DJANGO_SETTINGS_MODULE=dolphinop.settings</env>
    <module>django.core.handlers.wsgi:WSGIHandler()</module>
    <processes>4</processes>
    <master/>
    <!-- request timeout -->
    <harakiri>60</harakiri>
    <daemonize>/var/app/log/dolphinop-service/uwsgi.log</daemonize>
</uwsgi>
